<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:text="Phone XXX"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:id="@+id/textView22"
        android:layout_below="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

    <TextView
        android:text="Show XXX"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView33"
        android:layout_below="@+id/textView22"
        android:layout_alignStart="@+id/textView22" />

    <TextView
        android:text="From XXX"
        android:layout_width="wrap_content"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_height="wrap_content"
        android:id="@+id/textView44"
        android:layout_below="@+id/textView33"
        android:layout_alignStart="@+id/textView33" />

    <TextView
        android:text="Phone Number"
        android:layout_width="110dp"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtA"
        android:layout_alignParentStart="true"
        android:id="@+id/txtB" />

    <TextView
        android:text="DATE"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtC"
        android:id="@+id/txtD" />

    <TextView
        android:text="SQF REQ"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtD"
        android:id="@+id/txtE" />

    <TextView
        android:text="Show Name"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:id="@+id/txtC"
        android:layout_below="@+id/txtB" />

    <TextView
        android:text="SQF REQ XXX"
        android:layout_width="wrap_content"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_height="wrap_content"
        android:id="@+id/textView55"
        android:layout_below="@+id/textView44"
        android:layout_alignStart="@+id/textView44" />

    <TextView
        android:text="Client Name XXX"
        android:id="@+id/textView11"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp" />

    <TextView
        android:id="@+id/txtA"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:text="Person Name"
        android:layout_width="110dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView11"
        android:layout_alignBottom="@+id/textView11"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="To-date XXX"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="15dp"
        android:layout_toRightOf="@+id/textView5"
        android:layout_below="@+id/textView33"
        android:id="@+id/textView6" />

    <TextView
        android:text="To"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView5"
        android:paddingLeft="15dp"
        android:layout_toRightOf="@+id/textView44"
        android:layout_below="@+id/textView33"/>

    <Button
        android:text=""
        android:background="@android:color/transparent"
        android:drawableEnd="@drawable/info_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton2"
        android:layout_above="@+id/textView6"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="TextView"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10dp"
        android:layout_toEndOf="@+id/textView11"
        android:layout_alignBaseline="@+id/textView11"
        android:layout_toRightOf="@+id/textView11"
        android:layout_marginLeft="10dp"
        android:id="@+id/teextView" />

</RelativeLayout>